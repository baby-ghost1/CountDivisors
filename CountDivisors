#include <bits/stdc++.h>
using namespace std;

long long countDivisors(long long Num) {
    long long count = 0;
    for(long long i = 1; i*i <= Num; i++) {
        if(Num % i == 0) {
            if(i*i == Num) count++;   // perfect square
            else count += 2;         // pair of divisors i.e. i and Num/i
        }
    }
   return count;
}


int main() {
    int T; cin >> T;
    while(T--) {
        long long Num; cin >> Num;
        cout << countDivisors(Num) << endl;
    }
    return 0;
}
